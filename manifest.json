{
    "manifest_version": 3,
    "name": "Crunchyroll Single Anime Scraper",
    "description": "Scrapes an Anime and send the Data to API or Mongo",
    "version": "1.0",
    "icons": {
      "16": "images/cr.png",
      "48": "images/cr.png",
      "128": "images/cr.png"
    },
    "action": {
      "default_title": "CR S Anime Scraper"
    },
    "content_scripts": [
      {
        "matches": ["http://www.crunchyroll.com/*","https://www.crunchyroll.com/*"],
        "js": ["test.js"]
      }
    ]
  }